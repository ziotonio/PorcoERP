<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>

<hibernate-mapping>
    <class name="cn.itcast.invoice.auth.res.vo.ResModel" table="tbl_res">
        <id name="uuid">
            <generator class="native" />
        </id>
        <property name="name"/>
        <property name="url"/>
        
        <!-- 角色 -->
        <set name="roles" table="tbl_role_res">
        	<key column="resUuid"/>
        	<many-to-many
        		column="roleUuid"
        		class="cn.itcast.invoice.auth.role.vo.RoleModel"
        		/>
        </set>
    </class>
</hibernate-mapping>