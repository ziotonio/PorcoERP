<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>

<hibernate-mapping>
    <class name="cn.itcast.invoice.invoice.goods.vo.GoodsModel" table="tbl_goods">
        <id name="uuid">
            <generator class="native" />
        </id>
        <property name="name"/>
        <property name="origin"/>
        <property name="producer"/>
        <property name="unit"/>
        <property name="inPrice"/>
        <property name="outPrice"/>
        <property name="useNum"/>
        <property name="maxNum"/>
        <property name="minNum"/>

		<!-- 商品类别 -->
		<many-to-one 
			name="gtm"
			column="goodsTypeUuid"
			class="cn.itcast.invoice.invoice.goodstype.vo.GoodsTypeModel"
			/>

    </class>
</hibernate-mapping>